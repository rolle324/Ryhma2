<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.calcurator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CalCurator">
        <activity android:name=".history.HistoryDetails"
            android:parentActivityName=".history.History">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".history.History" />
        </activity>
        <activity android:name=".history.History" />
        <activity android:name=".userdata.Settings" />
        <activity android:name=".userdata.ActivitySelection" />
        <activity android:name=".userdata.WeightSelection" />
        <activity android:name=".userdata.HeightSelection" />
        <activity android:name=".userdata.AgeSelection" />
        <activity android:name=".userdata.GenderSelection" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>